@using ShoppingApp.Models
@model List<OrderStatus>
@{
    ViewBag.Title = "OrderStatus";
}


<h2>Order Status List </h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Order Id</th>
            <th>Order Name</th>
            <th>Actions</th>

        </tr>
    </thead>
    @foreach (var i in Model)
    {
        <tbody>
            <tr>
                <td>@i.Id</td>

                <td>@i.Name</td>
                <td>
                    <a href="/Order/OSView/@i.Id"><span class="glyphicon glyphicon-eye-open"></span> </a> |
                    <a href="/Order/OSEdit/@i.Id"><span class="glyphicon glyphicon-pencil"></span> </a> |
                    <a href="/Order/OSDelete/@i.Id"><span class="glyphicon glyphicon-trash"></span> </a>
                </td>
            </tr>

        </tbody>
    }
</table>