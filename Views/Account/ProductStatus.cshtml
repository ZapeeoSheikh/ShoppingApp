@using ShoppingApp.Models
@model List<ProductStatus>

@{
    ViewBag.Title = "ProductStatus";
}


<h2>Product Status List </h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Status</th>
            <th>Actions</th>

        </tr>
    </thead>
    @foreach (var i in Model)
    {
        <tbody>
            <tr>
                <td>@i.Id</td>

                <td>@i.Name</td>
                <td>
                    <a href="/Account/PSView"><span class="glyphicon glyphicon-eye-open"></span> </a>   |
                    <a href="/Account/PSEdit"><span class="glyphicon glyphicon-pencil"></span> </a>   |
                    <a href="/Account/PSDelete"><span class="glyphicon glyphicon-trash"></span> </a>
                </td>
            </tr>

        </tbody>
    }
</table>