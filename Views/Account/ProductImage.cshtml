@using ShoppingApp.Models
@model List<ProductImage>


<h2>Product Image List </h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Product Image Id</th>
            <th>Product Id</th>
            <th>Product Image</th>
        </tr>
    </thead>
    @foreach (var i in Model)
    {
        <tbody>
            <tr>
                <td>@i.Id</td>

                <td>@i.ProductId</td>

                <td>@i.Image</td>
            </tr>

        </tbody>
    }
</table>