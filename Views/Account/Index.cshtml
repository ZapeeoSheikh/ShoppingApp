
@{
    ViewBag.Title = "Index";
}

<h2>Index File </h2>



<div class="container col-lg-4 col-lg-push-2 col-lg-pull-3 ">
    <h2>Update DataBase Here</h2>
    <p>Click on the collapsible panel to open and close it.</p>
    <div class="panel-group">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse2">Update Product Table</a>
                </h4>
            </div>
            <div id="collapse2" class="panel-collapse collapse">



                <div class="container col-lg-10 col-lg-push-1">
                    <div class="panel  panel-primary ">
                        <div class="panel-heading">
                            Product Details
                        </div>
                        <div class="panel-body">
                            <form action="/Account/Product" method="post">
                                <div class="form-group">
                                    <label for="email"> Name :</label>
                                    <input type="text" class="form-control" id="email" name="Name">
                                </div>
                                <div class="form-group">
                                    <label for="desc">Description :</label>
                                    <input type="email" class="form-control" id="desc" name="Description">
                                </div>
                                <div class="form-group">
                                    <label for="phone">Price :</label>
                                    <input type="text" class="form-control" id="phone" name="Price">
                                </div>
                                <div class="form-group">
                                    <label for="pwd">Image :</label>
                                    <input type="password" class="form-control" id="pwd" name="Image">
                                </div>
                                <div class="form-group">
                                    <label for="si">Seller Id :</label>
                                    <input type="password" class="form-control" id="si" name="SellerId">
                                </div>
                                <div class="form-group">
                                    <label for="ps">Product Status :</label>
                                    <input type="password" class="form-control" id="ps" name="ProductStatus">
                                </div>
                                <button type="submit" class="btn btn-primary">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse1">Update Product Image Data </a>
                </h4>
            </div>
            <div id="collapse1" class="panel-collapse collapse">



                <div class="container col-lg-10 col-lg-push-1">
                    <div class="panel  panel-primary ">
                        <div class="panel-heading">
                            Product Images DataBase
                        </div>
                        <div class="panel-body panel-primary">
                            <form action="/Account/ProductImage" method="post">
                                <div class="form-group">
                                    <label for="img"> Image :</label>
                                    <input type="text" class="form-control" id="img" name="Image">
                                </div>
                                <div class="form-group">
                                    <label for="pi">Product Id :</label>
                                    <input type="text" class="form-control" id="pi" name="ProductId">
                                </div>
                                <button type="submit" class="btn btn-primary">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse3">Update Product Status Table</a>
                </h4>
            </div>
            <div id="collapse3" class="panel-collapse collapse">



