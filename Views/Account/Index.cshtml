
@{
    ViewBag.Title = "Index";
}





<div class="container-fluid">
    <div class="container-fluid">
        <h2 class="jumbotron">
            Update DataBase Here
        </h2>
        <p class="text-warning">Click on the collapsible panel to open and close it.</p>

    </div>
    <div class="panel-group ">
        <div class="col-lg-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse6">Roles Form</a>
                    </h4>
                </div>
                <div id="collapse6" class="panel-collapse collapse">



                    <div class="panel  panel-default ">
                        <div class="panel-heading">
                            Set Different Roles
                        </div>
                        <div class="panel-body">
                            <form action="/Order/Role" method="post">
                                <div class="form-group">
                                    <label for="name"> Role Title :</label>
                                    <input type="text" class="form-control" id="name" name="Name">
                                </div>

                                <button type="submit" class="btn btn-primary">Add Now</button>
                            </form>
                        </div>
                    </div>





                </div>
            </div>
        </div>

        <div class="col-lg-3">
            <div class="panel panel-danger">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse3">Product Status Form</a>
                    </h4>
                </div>
                <div id="collapse3" class="panel-collapse collapse">



                    <div class="panel  panel-primary ">
                        <div class="panel-heading">
                            Set Product Status
                        </div>
                        <div class="panel-body">
                            <form action="/Account/ProductStatus" method="post">
                                <div class="form-group">
                                    <label for="name"> Product Name :</label>
                                    <input type="text" class="form-control" id="name" name="Name">
                                </div>

                                <button type="submit" class="btn btn-primary">Add Now</button>
                            </form>
                        </div>
                    </div>





                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="panel panel-warning">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse7">User Form </a>
                    </h4>
                </div>
                <div id="collapse7" class="panel-collapse collapse">



                    <div class="panel  panel-success ">
                        <div class="panel-heading">
                            Set User Details
                        </div>
                        <div class="panel-body">
                            <form action="/Order/User" method="post">
                                <div class="form-group">
                                    <label for="name"> User Name :</label>
                                    <input type="text" class="form-control" id="name" name="Name">
                                </div>
                                <div class="form-group">
                                    <label for="email"> User Email :</label>
                                    <input type="text" class="form-control" id="email" name="Email">
                                </div>
                                <div class="form-group">
                                    <label for="pwd"> User Password :</label>
                                    <input type="password" class="form-control" id="pwd" name="Password">
                                </div>
                                <div class="form-group">
                                    <label for="role"> User Role Id :</label>
                                    <input type="number" class="form-control" id="role" name="RoleId">
                                </div>

                                <button type="submit" class="btn btn-danger">Add Now</button>
                            </form>
                        </div>
                    </div>





                </div>
            </div>

        </div>
        <div class="col-lg-3">
            <div class="panel panel-primary ">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse2">Products Form</a>
                    </h4>
                </div>
                <div id="collapse2" class="panel-collapse collapse">



                    <div class="panel panel-success ">
                        <div class="panel-heading">
                            Set Product Details
                        </div>
                        <div class="panel-body">
                            <form action="/Account/Product" method="post">
                                <div class="form-group">
                                    <label for="email">Product Name :</label>
                                    <input type="text" class="form-control" id="email" name="Name">
                                </div>
                                <div class="form-group">
                                    <label for="ps">Product Status Id:</label>
                                    <input type="number" class="form-control" id="ps" name="ProductStatusId">
                                </div>
                                <div class="form-group">
                                    <label for="desc">Product Description :</label>
                                    <input type="text" class="form-control" id="desc" name="Description">
                                </div>
                                <div class="form-group">
                                    <label for="phone">Product Price :</label>
                                    <input type="text" class="form-control" id="phone" name="Price">
                                </div>
                                <div class="form-group">
                                    <label for="pwd">Product Image :</label>
                                    <input type="file" class="form-control" id="pwd" name="Image">
                                </div>
                                <div class="form-group">
                                    <label for="si">Seller Id :</label>
                                    <input type="number" class="form-control" id="si" name="SellerId">
                                </div>
                                <button type="submit" class="btn btn-success">Add Now</button>
                            </form>
                        </div>
                    </div>





                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="panel panel-warning">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse1">Product Images Form</a>
                    </h4>
                </div>
                <div id="collapse1" class="panel-collapse collapse">



                    <div class="panel  panel-danger ">
                        <div class="panel-heading">
                            Set Product Images
                        </div>
                        <div class="panel-body ">
                            <form action="/Account/ProductImage" method="post">
                                <div class="form-group">
                                    <label for="pi">Product Id :</label>
                                    <input type="number" class="form-control" id="pi" name="ProductId">
                                </div>
                                <div class="form-group">
                                    <label for="img">Product Image :</label>
                                    <input type="file" class="form-control" id="img" name="Image">
                                </div>
                                <button type="submit" class="btn btn-danger">Add Now</button>
                            </form>
                        </div>
                    </div>





                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="panel panel-success">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse4">Orders Form</a>
                    </h4>
                </div>
                <div id="collapse4" class="panel-collapse collapse">



                    <div class="panel  panel-warning ">
                        <div class="panel-heading">
                            Set Order Details
                        </div>
                        <div class="panel-body ">
                            <form action="/Order/Orders" method="post">
                                <div class="form-group">
                                    <label for="date"> Date and Time :</label>
                                    <input type="datetime-local" class="form-control" id="date" name="DateTime">
                                </div>
                                <div class="form-group">
                                    <label for="cost">Order Status Id :</label>
                                    <input type="number" class="form-control" id="cost" name="OrderStatusId">
                                </div>
                                <div class="form-group">
                                    <label for="des">Buyer Id :</label>
                                    <input type="number" class="form-control" id="des" name="BuyerId">
                                </div>
                                <div class="form-group">
                                    <label for="de">Product Id :</label>
                                    <input type="number" class="form-control" id="de" name="ProductId">
                                </div>
                                <button type="submit" class="btn btn-warning">Add Now</button>
                            </form>
                        </div>
                    </div>





                </div>
            </div>
        </div>
        <div class="col-lg-3">

            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse5">Order Status Form</a>
                    </h4>
                </div>
                <div id="collapse5" class="panel-collapse collapse">



                    <div class="panel  panel-primary ">
                        <div class="panel-heading">
                            Set Order Status
                        </div>
                        <div class="panel-body panel-primary">
                            <form action="/Order/OrderStatus" method="post">
                                <div class="form-group">
                                    <label for="cnic">Order Status Title :</label>
                                    <input type="text" class="form-control" id="cnic" name="Name">
                                </div>

                                <button type="submit" class="btn btn-primary">Add Now</button>
                            </form>
                        </div>
                    </div>





                </div>
            </div>
        </div>
    </div>
</div>