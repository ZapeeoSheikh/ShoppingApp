                    <div class="panel  panel-primary ">
                        <div class="panel-heading">
                            Set Orders Details
                        </div>
                        <div class="panel-body panel-primary">
                            <form action="/Order/Order" method="post">
                                <div class="form-group">
                                    <label for="date"> Date and Time :</label>
                                    <input type="datetime" class="form-control" id="date" name="DateTime">
                                </div>
                                <div class="form-group">
                                    <label for="cost">Order Status Id :</label>
                                    <input type="text" class="form-control" id="cost" name="OrderStatusId">
                                </div>
                                <div class="form-group">
                                    <label for="des">Buyer Id :</label>
                                    <input type="text" class="form-control" id="des" name="BuyerId">
                                </div>
                                <div class="form-group">
                                    <label for="des">Product Id :</label>
                                    <input type="text" class="form-control" id="des" name="ProductId">
                                </div>
                                <button type="submit" class="btn btn-primary">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
